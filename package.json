  "homepage": "https://github.com/limi58/ease-animate#readme",
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "chai": "^3.5.0",
    "webpack": "^1.13.1"
  },
